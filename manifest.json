{
    "manifest_version": 2,
    "name": "Clipper",
    "version": "1.0",
  
    "description": "Bypasses clipboard restrictions on webpages",
  
    "icons": {
      "48": "clipper.svg"
    },
  
    "browser_action": {
      "default_icon": "clipper.svg",
      "default_title": "Clipper",
      "default_popup": "popup/popup.html"
    },
    "permissions": [
      "tabs",
      "storage"
    ]
  }
  